define(function(){return{PreviewImage:function(e,a,n){var r=e.value.substring(e.value.lastIndexOf(".")+1).toLowerCase(),t=(window.navigator.userAgent.toUpperCase(),$(a));-1<"jpg,gif,png".indexOf(r)?$(n).ajaxSubmit({url:"http://download.csdn.net/index.php/sources_album/getalbumimg",data:{imagesrc:e.value},dataType:"json",success:function(e){0!==e.succ?t.attr("src",e.url):alert(e.errmsg)},error:function(e){}}):(alert("仅支持"+allowSuffix+"为后缀名的文件!"),e.value="")}}});