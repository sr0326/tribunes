define(["apps/_verificaUser"],function(t){return{init:function(){var a=$(".praise_btn"),s=a.attr("data-id");a.on("click",function(){var e=$(this),r=parseInt(e.next("em").text());if(t._verificaUser()){$(this).addClass("praise_already");var a="/index.php/album/do_download_favorite/"+s;$.ajax({type:"get",url:a,async:!1,dataType:"jsonp",jsonp:"callback",success:function(a){1==a.succ?e.next("em").text(r+1):-1==a.succ?alert(a.msg):0==a.succ?alert(a.msg):-3==a.succ&&(alert(a.msg),$(this).removeClass("praise_already"))},error:function(){console.log("Send Ajax error ...")}})}})}}});